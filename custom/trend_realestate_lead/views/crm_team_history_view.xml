<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="crm_team_history_form_view" model="ir.ui.view">
            <field name="name">crm.team.history.form.view</field>
            <field name="model">crm.team.history</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group col="4">
                            <field name="name" readonly="1"/>
                            <field name="date" readonly="1"/>
                            <field name="change_from_user_id" readonly="1"/>
                            <field name="change_to_user_id" readonly="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="crm_team_history_tree_view" model="ir.ui.view">
            <field name="name">crm.team.history.tree.view</field>
            <field name="model">crm.team.history</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" readonly="1"/>
                    <field name="date" readonly="1"/>
                    <field name="change_from_user_id" readonly="1"/>
                    <field name="change_to_user_id" readonly="1"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>

<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="available_units_wizard_wizard_form" model="ir.ui.view">
            <field name="name">available.units.wizard.wizard.form</field>
            <field name="model">available.units.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <h3>
                        choose unit to proceed with
                    </h3>
                    <field name="product_ids"/>
                    <footer>
                        <button name="confirm_unit" string="confirm unit" type="object"
                                class="oe_highlight"/>
                        or
                        <button string="Close" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
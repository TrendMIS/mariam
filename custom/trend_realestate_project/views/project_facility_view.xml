<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="project_facility_form_View">
            <field name="name">project.facility.form.view</field>
            <field name="model">project.facility</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="project_facility_tree_View">
            <field name="name">project.facility.tree.view</field>
            <field name="model">project.facility</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="project_facility_action">
            <field name="name">Real Estate Facility</field>
            <field name="res_model">project.facility</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
    </data>
</odoo>
<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="unit_block_view_tree" model="ir.ui.view">
            <field name="name">unit_block_view_tree</field>
            <field name="model">realestate.unit.block</field>
            <field name="arch" type="xml">
                <tree string="unit_block_tree">
                    <field name="number"/>
                    <field name="project_id"/>
                </tree>
            </field>
        </record>

        <record id="unit_view_view_tree" model="ir.ui.view">
            <field name="name">unit_view_view_tree</field>
            <field name="model">realestate.unit.view</field>
            <field name="arch" type="xml">
                <tree string="unit_view_tree">
                    <field name="name"/>
                    <field name="project_id"/>
                </tree>
            </field>
        </record>

        <record id="unit_finish_view_tree" model="ir.ui.view">
            <field name="name">unit_finish_view_tree</field>
            <field name="model">realestate.unit.finish</field>
            <field name="arch" type="xml">
                <tree string="unit_finish_tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="unit_block_action" model="ir.actions.act_window">
            <field name="name">Project Blocks</field>
            <field name="res_model">realestate.unit.block</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="unit_view_action" model="ir.actions.act_window">
            <field name="name">Project Views</field>
            <field name="res_model">realestate.unit.view</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="unit_finish_action" model="ir.actions.act_window">
            <field name="name">Finish statues</field>
            <field name="res_model">realestate.unit.finish</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>